package psp.ud1_1_6.multiproceso.ejemplo.bucles;

public class HelloProcess1 {
    public static void main(String[] args) {
        for (int i = 0; i < 5; i++) {
            System.out.println("Proceso 1 - Valor: " + i);
            try {
                Thread.sleep(1000);
            } catch (InterruptedException e) {
                throw new RuntimeException(e);
            }
        }
    }
}
